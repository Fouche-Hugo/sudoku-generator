{
  "name": "sudoku-generator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p postcss:watch vite:dev",
    "vite:dev": "vite",
    "build": "npm-run-all postcss:build vite:build",
    "vite:build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --ext .js,.ts,.svelte src",
    "lint:css": "stylelint .svelte,.css --fix",
    "postcss:build": "postcss src/style/source --dir src/style/output",
    "postcss:watch": "postcss src/style/source --dir src/style/output --watch"
  },
  "devDependencies": {
    "@html-eslint/eslint-plugin": "^0.16.0",
    "@html-eslint/parser": "^0.16.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.3",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-svelte": "^2.9.0",
    "stylelint": "^15.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^30.0.1",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
